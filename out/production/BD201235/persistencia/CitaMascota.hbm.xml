<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="persistencia">
    <class name="Cita" table="cita">
        <id name="idcita" column="idcita" type="java.lang.Integer" unsaved-value="null" >
            <generator class="native"> </generator>
        </id>

        <property name="fecha" column="fecha"/>
        <property name="idmascota" column="idmascota"/>
        <property name="idservicio" column="idservicio"/>


    </class>
</hibernate-mapping>